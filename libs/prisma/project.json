{
  "name": "prisma",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/prisma/src",
  "projectType": "library",
  "targets": {
    "db-push": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma db push --schema src/lib/schema.prisma",
        "cwd": "libs/prisma"
      }
    },
    "generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma generate --schema src/lib/schema.prisma",
        "cwd": "libs/prisma"
      }
    },
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/prisma",
        "main": "libs/prisma/src/index.ts",
        "tsConfig": "libs/prisma/tsconfig.lib.json",
        "assets": ["libs/prisma/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/prisma/**/*.ts"]
      }
    }
  },
  "tags": []
}
